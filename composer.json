{
    "name": "greg0x46/masked-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova",
        "mask"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Greg0x46\\MaskedField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Greg0x46\\MaskedField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
